clc; 
clear; 
// Given data 
d1 = 4; 
d2 = 4; 
d3 = 8; 
dia = 2e-2; 
r = dia / 2; 
Vline = 132; 
f = 50; 
length_km = 100; 
// spacing D12 in meters 
// spacing D23 in meters 
// spacing D31 in meters 
// diameter in meters 
// radius in meters 
// kV 
// Hz 
epsilon0 = 8.854e-12; 
// km 
// F/m 
// Equivalent spacing (transposed line) 
Deq = (d1 * d2 * d3)^(1/3); 
// Capacitance per phase per meter 
C_per_m = 2 * %pi * epsilon0 / log(Deq / r); 
disp("Capacitance per phase per meter (F/m) = " + string(C_per_m)); 
// Capacitance per phase per km 
C_per_km = C_per_m * 1000; 
disp("Capacitance per phase per km (F/km) = " + string(C_per_km)); 
// Capacitance per phase for 100 km 
C_100km = C_per_km * length_km; 
disp("Capacitance per phase for 100 km (F) = " + string(C_100km)); 
// Phase voltage 
V_phase = (Vline * 1000) / sqrt(3); 
// Charging current per phase 
Ic = 2 * %pi * f * C_100km * V_phase; 
disp("Charging current per phase (A) = " + string(Ic));
