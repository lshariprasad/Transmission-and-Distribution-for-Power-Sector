clc;
clear;

d = input('Enter the spacing between conductor in centimeter: ');
r = input('Enter the radius of the conductor in centimeter: ');

// Inductance per meter
Ip = 1e-7 * (0.5 + 2 * log(d / r));
disp(Ip, 'The inductance of 3-phase symmetrical transmission line per meter (H/m): ');

// Inductance per kilometer
Ip_km = Ip * 1000;
disp(Ip_km, 'Inductance per kilometer (H/km): ');
