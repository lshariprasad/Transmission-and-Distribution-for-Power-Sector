clc;
clear;

L = input("Enter span length (m) = ");
Tu = input("Enter ultimate strength = ");
FOS = input("Enter factor of safety = ");

// Working tension
T = Tu / FOS;

// Weight range
W = 0.68:0.02:0.9;

// Sag calculation
S = (W .* L^2) / (8 * T);

// Plot
plot(W, S);
xlabel("Weight of conductor (kg)");
ylabel("Sag (m)");
title("Calculation of Sag with Various Conductor Weights");

// Display sag values
disp("Sag values (m) = " + string(S));
